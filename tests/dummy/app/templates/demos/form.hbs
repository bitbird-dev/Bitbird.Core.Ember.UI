form

{{tr-toggle 
      label="isFullScreen" 
      selectedValue=formSettings.isFullScreen 
      isDisabled=false 
      postfix=(if psSwValue "true" "false")}}
{{tr-toggle 
      label="section" 
      selectedValue=formSettings.section 
      isDisabled=false 
      postfix=(if psSwValue "true" "false")}}
{{tr-toggle 
      label="escape" 
      selectedValue=formSettings.escape 
      isDisabled=false 
      postfix=(if psSwValue "true" "false")}}
{{input type="range" min="1" max="3" class="slider" value=formSettings.size}} {{formSettings.size}}
{{#tr-form header="tr-form" 
  isFullScreen=formSettings.isFullScreen 
  section=formSettings.section
  escape=formSettings.escape 
  size=formSettings.size}}
  {{#each model as |item|}}
    {{#tr-form-block 
      header=item.header as |formBlock|}}
      {{#formBlock.content}}
        <ul>
          <li>id={{item.id}}</li>
          <li>size {{input type="range" min="1" max="3" class="slider" value=item.size}}</li>
          <li>useCustomMenu {{input type="checkbox" class="checkbox" checked=item.useCustomMenu}}</li>
          <li>fullSizeContent {{input type="checkbox" class="checkbox" checked=item.fullSizeContent}}</li>
          <li>canMaximize {{input type="checkbox" class="checkbox" checked=item.canMaximize}}</li>
          <li>canCollapse {{input type="checkbox" class="checkbox" checked=item.canCollapse}}</li>
          <li>overflow <span>{{simple-combobox options=overflowOptions}}</span></li>
        </ul>
      {{/formBlock.content}}
    {{/tr-form-block}}
  {{/each}}
{{/tr-form}}